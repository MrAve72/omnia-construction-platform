{
  "builds": [
    {
      "src": "front-site-alex/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "front-site-alex/api/contact.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    { "source": "/api/contact", "destination": "/api/contact" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
