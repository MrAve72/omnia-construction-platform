{
  "builds": [
    {
      "src": "front-site-alex/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "front-site-alex/api/contact.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/contact",
      "dest": "front-site-alex/api/contact.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
